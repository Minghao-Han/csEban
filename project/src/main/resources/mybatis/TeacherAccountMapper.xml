<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace=绑定一个对应的Dao/Mapper接口-->
<mapper namespace="com.AAA.csEban.mapper.TeacherAccountMapper">

    <select id="selectAccountById" resultType="com.AAA.csEban.pojo.TeacherAccount">
        select *
        from teacher_account T
        where T.teacher_id = #{id};
    </select>
</mapper>